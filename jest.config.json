{
  "collectCoverage": false,
  "collectCoverageFrom": ["**/*.{ts,tsx}", "!**/*.d.ts", "!**/index.ts", "!**/start*.ts", "!**/webpack.config.ts"],
  "coverageReporters": ["html", "lcov", "text"],
  "coverageThreshold": {
    "global": {
      "branches": 10,
      "functions": 10,
      "lines": 10,
      "statements": 10
    }
  },
  "preset": "ts-jest",
  "setupFilesAfterEnv": [],
  "testEnvironment": "jsdom",
  "testRegex": "\\.test\\.tsx?$"
}
